<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:util="http://www.springframework.org/schema/util"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
        http://www.springframework.org/schema/util
        http://www.springframework.org/schema/util/spring-util-4.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.2.xsd"
>

    <bean id="user1" class="com.epam.reshetnev.spring.core.domain.User">
        <constructor-arg value="${kim1.name}"/>
        <constructor-arg value="${kim1.email}"/>
        <!--constructor-arg value="${kim1.birthDay}"/-->
    </bean>
    <bean id="user2" class="com.epam.reshetnev.spring.core.domain.User">
        <constructor-arg value="${kim2.name}"/>
        <constructor-arg value="${kim2.email}"/>
        <!--constructor-arg value="${kim2.birthDay}"/-->
    </bean>
    <bean id="user3" class="com.epam.reshetnev.spring.core.domain.User">
        <constructor-arg value="${ivan.name}"/>
        <constructor-arg value="${ivan.email}"/>
        <!--constructor-arg value="${ivan.birthDay}"/-->
    </bean>

    <util:list id="userList">
        <ref bean="user1"/>
        <ref bean="user2"/>
        <ref bean="user3"/>
    </util:list>

</beans>